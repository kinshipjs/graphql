{
    "name": "@kinshipjs/graphql",
    "version": "0.0.1",
    "homepage": "https://github.com/kinshipjs/graphql",
    "repository": {
        "type": "git",
        "url": "https://github.com/kinshipjs/graphql"
    },
    "description": "Plugin that converts existing MyORM contexts into GraphQL endpoints.",
    "type": "module",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/types/index.d.ts",
    "scripts": {
        "clean": "del /S /Q .\\dist",
        "build": "npm run test && npm run clean && npm run build:esm && npm run build:cjs",
        "build:esm": "tsc -p ./configs/tsconfig.esm.json",
        "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
        "test": "node ./tests/test.js",
        "prepack": "npm run build"
    },
    "keywords": [
        "kinshipjs",
        "graphql",
        "typescript",
        "table",
        "left join",
        "query",
        "insert",
        "update",
        "delete"
    ],
    "author": "Travis Zuleger",
    "license": "(MIT OR Apache-2.0)",
    "dependencies": {
        "@kinshipjs/core": "file:../core",
        "graphql": "^16.8.1",
        "pluralize": "^8.0.0"
    },
    "devDependencies": {
        "@kinshipjs/json": "^0.0.9-development",
        "@types/express": "^4.17.21",
        "@types/node": "^20.3.1",
        "dotenv": "^16.0.3",
        "typescript": "^4.9.4"
    },
    "files": [
        "dist/**/*"
    ],
    "exports": {
        ".": {
            "import": {
                "types": "./dist/esm/types/index.d.ts",
                "default": "./dist/esm/index.js"
            },
            "require": {
                "types": "./dist/cjs/types/index.d.ts",
                "default": "./dist/cjs/index.js"
            }
        }
    }
}
